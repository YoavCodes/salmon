{
	"web": {
		"type": "http",
		"exec": [{
			"lang": "nodejs",
			"version": "0.10.28",
			"main": "./web/salmon.js",
			"watch": ["./"],
			"exclude": ["./web/salmon/node_modules", "./web/libs/node_modules", "./web/public"],
			"cachecontrol": ["./web/public", "./web/template"],
			"gruntdirectory": "./web/salmon",
			"npm": ["./web/salmon", "./web/libs"],
			"endpoints": [{"path":"/0", "port": "8080"}]			
		}],
		"static": [{"path": "/", "dir": "./public"}], 
		"domains": {"app1.test": []},
		"instances": ["6000"]
	}
}