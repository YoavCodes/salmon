{
	"web": {
		"type": "http",
		"exec": [{
			"lang": "nodejs",
			"version": "0.10.28",
			"main": "./web/salmon.js",
			"watch": ["./"],
			"exclude": ["./.git","./web/salmon/.grunt", "./web/salmon/_SpecRunner.html", "./web/salmon/node_modules", "./web/libs/node_modules"],
			"less": [ {"from": "./web/public/assets/less", "to": "./web/public/assets/css", "min": false} ],
			"jasmine": {"frontend": "./web/test/fin", "backend": "./web/test/tail"},
			"cachecontrol": ["./web/public", "./web/template"],			
			"npm": ["./web/salmon", "./web/libs"],
			"endpoints": [{"path":"/0", "port": "8080"}]			
		}],
		"static": [{"path": "/", "dir": "./public"}], 
		"domains": {"app1.test": []},
		"instances": ["6000"]
	}
}